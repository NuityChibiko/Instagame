<%- include("patials/header") %>

้<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>teams</title>
    <link rel="stylesheet" href="/css/team.css">

    <script src="./js/load_more.js"></script>
    <!------ Include the above in your HEAD tag ---------->

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="http://getbootstrap.com/dist/js/bootstrap.min.js"></script>
</head>

<body>
    <main class="teams-body" role="news"> 
        <div id="teams-section" style="position: relative;">
          <header class="teams__headline headline-sticky healine-uprank">
            <h2 class="headline__name">รายชื่อที่ต้องการอัพเกรดระดับสมาชิก</h2>
          </header>
  
          <section class="box">
            <div class="row" style="display:flex; flex-wrap:wrap;">
                <article class="entry" style="display: none;">
                    <table class="table uprank-table">
                        <thead class="uprank-head">
                          <tr>
                            <th scope="col">ชื่อผู้ใช้</th>
                            <th scope="col">ชื่อบริษัท</th>
                            <th scope="col">ไฟล์ PDF</th>
                            <th scope="col">การอนุมัติ</th>
                            <th scope="col">การปฏิเสธ</th>
                          </tr>
                        </thead>
                        <% List.forEach(function(list){ %>
                        <tbody class="uprank-item">
                          <tr>
                            <td><%= list.list.username %></td>
                            <td><%= list.company %></td>
                            <td><a href="/uploads/user/<%= list.doc %>"><%= list.doc %></a></td>
                            <td>
                                <form action="/promotion/success/<%= list._id %>" method="POST">
                                    <button class="entry__meta__accept">
                                        <i class="fas fa-check-circle"></i>
                                        อนุมัติ
                                    </button>
                                </form>
                            </td>
                            <td>
                                <form action="/promotion/denied/<%= list._id %>?_method=DELETE" method="POST">
                                <button class="entry__meta__cancle">
                                    <i class="fas fa-times-circle"></i>
                                    ปฏิเสธ
                                </button>
                            </form>
                            </td>
                          </tr>
                        </tbody>
                        <% }) %>
                      </table>
                </article> 
            </div>
          </section>
      </div>
    </main>
</body>

    
<%- include("patials/footer") %>